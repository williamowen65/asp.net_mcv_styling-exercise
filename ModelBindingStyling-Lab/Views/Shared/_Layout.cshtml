<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ModelBindingStyling_Lab</title>
    <script src="https://kit.fontawesome.com/16aebb51db.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ModelBindingStyling_Lab.styles.css" asp-append-version="true" />
    @RenderSection("Styles", required: false)
</head>
<body>
    <header class="d-flex align-items-center">

        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">ModelBindingStyling_Lab</a>
        <style>
            /* Style written here to override */
            .disabled-link a {
                text-decoration: none !important;
            }
        </style>
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0">
                <li class="breadcrumb-item active disabled-link"><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                <li id="user-profile-breadcrumb" class="breadcrumb-item d-none"><a asp-area="" asp-controller="Home" asp-action="UserProfile">User Profile</a></li>
                <li id="printer-list-breadcrumb" class="breadcrumb-item d-none" aria-current="page">Printer List</li>
            </ol>
        </nav>

    </header>
    <div class="">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - ModelBindingStyling_Lab
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
